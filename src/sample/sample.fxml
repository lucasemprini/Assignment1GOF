<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="525.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.SampleController">
  <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
        <BorderPane fx:id="buttonPane">
          <center>
            <Label fx:id="generationsLabel" maxWidth="224.0" text="Generation :" />
          </center>
          <left>
            <Button fx:id="buttonStart" mnemonicParsing="false" prefHeight="33.0" prefWidth="92.0" text="START" />
          </left>
          <right>
            <Button fx:id="buttonStop" mnemonicParsing="false" prefHeight="33.0" prefWidth="92.0" text="STOP" />
          </right>
        </BorderPane>
        <padding>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
      </HBox>
    </bottom>
    <center>
        <AnchorPane>
            <ScrollPane  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
                         fx:id="scrollPane" fitToHeight="true" fitToWidth="true">
                <GridPane fx:id="gameGrid"/>
            </ScrollPane>
        </AnchorPane>
    </center>
  </BorderPane>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</AnchorPane>
